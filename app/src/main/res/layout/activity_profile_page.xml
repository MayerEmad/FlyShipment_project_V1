<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#30555C">


    <TextView
        android:id="@+id/profile_passport_edit_text"
        android:layout_width="210dp"
        android:layout_height="50dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical|end"
        android:text=""
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_phone_edit_text" />

    <CheckedTextView
        android:id="@+id/profile_passport_check_text"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="7dp"
        android:checked="true"
        android:gravity="center_vertical"
        android:text="@string/passport_num_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_phone_check_text" />

    <TextView
        android:id="@+id/profile_phone_edit_text"
        android:layout_width="220dp"
        android:layout_height="50dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical|end"
        android:text=""
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_email_edit_text" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="@string/shipments_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="17sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/profile_trips_number_textview"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:gravity="center"
        android:text="@string/trips_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="17sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/profile_shipments_number_textview"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/profile_trips_number_textview"
        android:layout_width="50dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:gravity="center"
        android:text="0"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="19sp"
        app:layout_constraintStart_toEndOf="@+id/profile_image_image_view"
        app:layout_constraintTop_toBottomOf="@+id/profile_shipments_number_textview" />

    <TextView
        android:id="@+id/profile_shipments_number_textview"
        android:layout_width="50dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:gravity="center"
        android:text="0"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="19sp"
        app:layout_constraintStart_toEndOf="@+id/profile_image_image_view"
        app:layout_constraintTop_toBottomOf="@+id/profile_deals_number_textview" />

    <ImageButton
        android:id="@+id/profile_back_button"
        android:layout_width="65dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/bushape"
        android:foreground="@drawable/ic_arrow_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_user_name_text_view"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:gravity="center"
        android:text="@string/user_name_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_image_image_view" />

    <Button
        android:id="@+id/profile_edit_button"
        android:layout_width="159dp"
        android:layout_height="48dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/bushape"
        android:text="@string/edit_word"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/profile_image_image_view"
        android:layout_width="189dp"
        android:layout_height="186dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="108dp"
        android:foreground="@drawable/add_photo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/profile_deals_number_textview"
        android:layout_width="50dp"
        android:layout_height="35dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="140dp"
        android:gravity="center"
        android:text="0"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="19sp"
        app:layout_constraintStart_toEndOf="@+id/profile_image_image_view"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginTop="140dp"
        android:gravity="center"
        android:text="@string/deals_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="17sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/profile_deals_number_textview"
        app:layout_constraintTop_toTopOf="parent" />

    <CheckedTextView
        android:id="@+id/profile_email_check_text"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="60dp"
        android:checked="true"
        android:gravity="center_vertical"
        android:text="@string/email_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_user_name_text_view" />

    <CheckedTextView
        android:id="@+id/profile_phone_check_text"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="7dp"
        android:checked="true"
        android:gravity="center_vertical"
        android:text="@string/phone_num_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_email_check_text" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="40dp"
        android:layout_height="24dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:text="@string/rate_word"
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/profile_image_image_view"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <RatingBar
        android:id="@+id/profile_ratingbar"
        style="?android:attr/ratingBarStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:foregroundGravity="center_vertical"
        android:numStars="5"
        android:rating="4.3"
        android:stepSize="0.1"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/profile_email_edit_text"
        android:layout_width="240dp"
        android:layout_height="50dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical|end"
        android:text=""
        android:textColor="#FEFEFE"
        android:textColorHint="#FEFEFE"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_user_name_text_view" />

</androidx.constraintlayout.widget.ConstraintLayout>